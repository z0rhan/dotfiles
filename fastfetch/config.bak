{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "kitty",
    "source": "~/Pictures/fastfetch/alucard.png",
    "height": 20,
    "width": 50,
    "padding": {
        "top": 3,
        "left": 3
    },
  },
  "modules": [
    "break",
    {
      "type": "custom",
      "format": "\u001b[31m┌──────────────────────Hardware──────────────────────┐",
    },
    {
      "type": "host",
      "key": " PC",
      "keyColor": "red",
    },
    {
      "type": "cpu",
      "key": "│ ├",
      "showPeCoreCount": true,
      "keyColor": "red",
    },
    {
      "type": "gpu",
      "key": "│ ├󰍛",
      "keyColor": "red",
    },
    {
      "type": "display",
      "key": "│ ├󰍛",
      "keyColor": "red",
    },
    {
      "type": "memory",
      "key": "│ ├󰍛",
      "keyColor": "red",
    },
    {
      "type": "disk",
      "key": "└ └",
      "keyColor": "red",
    },
    {
      "type": "custom",
      "format": "\u001b[31m└────────────────────────────────────────────────────┘",
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[31m┌──────────────────────Software──────────────────────┐",
    },
    {
      "type": "os",
      "key": " OS",
      "keyColor": "red",
    },
    {
      "type": "kernel",
      "key": "│ ├",
      "keyColor": "red",
    },
    {
      "type": "packages",
      "key": "│ ├󰏖",
      "keyColor": "red",
    },
    {
      "type": "shell",
      "key": "└ └",
      "keyColor": "red",
    },
    "break",
    {
      "type": "de",
      "key": " DE",
      "keyColor": "red",
    },
    {
      "type": "lm",
      "key": "│ ├",
      "keyColor": "red",
    },
    {
      "type": "wm",
      "key": "│ ├",
      "keyColor": "red",
    },
    {
      "type": "wmtheme",
      "key": "│ ├󰉼",
      "keyColor": "red",
    },
    {
      "type": "gpu",
      "key": "└ └󰍛",
      "format": "{3}",
      "keyColor": "red",
    },
    {
      "type": "custom",
      "format": "\u001b[31m└────────────────────────────────────────────────────┘",
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[31m┌────────────────────Uptime / Age────────────────────┐",
    },
    {
      "type": "command",
      "key": "  OS Age ",
      "keyColor": "red",
      "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days",
    },
    {
      "type": "uptime",
      "key": "  Uptime ",
      "keyColor": "red",
    },
    {
      "type": "custom",
      "format": "\u001b[31m└────────────────────────────────────────────────────┘",
    },
    "break",
  ],
}

